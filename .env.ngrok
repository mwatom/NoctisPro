# NoctisPro Ngrok Environment Configuration
# Configure your ngrok settings here

# Ngrok Auth Token (get from https://dashboard.ngrok.com/get-started/your-authtoken)
# NGROK_AUTHTOKEN=your_auth_token_here

# Static Domain Configuration (for paid accounts)
# NGROK_USE_STATIC=false  # Set to true to use static subdomain/domain
# NGROK_SUBDOMAIN=noctispro  # Your preferred subdomain (requires paid account)
# NGROK_DOMAIN=noctis.yourdomain.com  # Your custom domain (requires paid account + verification)

# Ngrok Region (us, eu, ap, au, sa, jp, in)
NGROK_REGION=us

# Default tunnel name to use
NGROK_TUNNEL_NAME=noctispro-http

# Ngrok Web Interface
NGROK_WEB_ADDR=localhost:4040

# Server configuration
DJANGO_PORT=8000
DJANGO_HOST=0.0.0.0

# Django settings for ngrok
ALLOWED_HOSTS="*"  # Allow all hosts for ngrok compatibility
DEBUG=False
USE_TZ=True
SECRET_KEY="noctis-pro-ngrok-secret-key-change-in-production"